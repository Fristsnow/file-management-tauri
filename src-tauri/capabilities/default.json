{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "fs:allow-write-file",
      "allow": [
        {
          "path": "$DOWNLOAD/*"
        },
        {
          "path": "$HOME/*"
        },
        {
          "path": "$DESKTOP/*"
        },
        {
          "path": "$DOCUMENT/*"
        },
        {
          "path": "**"
        }
      ]
    },
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        {
          "path": "$DOWNLOAD/*"
        },
        {
          "path": "$HOME/*"
        },
        {
          "path": "$DESKTOP/*"
        },
        {
          "path": "$DOCUMENT/*"
        },
        {
          "path": "**"
        }
      ]
    },
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "$DOWNLOAD/*"
        },
        {
          "path": "$HOME/*"
        },
        {
          "path": "$DESKTOP/*"
        },
        {
          "path": "$DOCUMENT/*"
        },
        {
          "path": "**"
        }
      ]
    },
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        {
          "path": "$DOWNLOAD/*"
        },
        {
          "path": "$HOME/*"
        },
        {
          "path": "$DESKTOP/*"
        },
        {
          "path": "$DOCUMENT/*"
        },
        {
          "path": "**"
        }
      ]
    },
    "dialog:allow-open",
    "dialog:default",
    "fs:allow-read-dir",
    "fs:allow-stat",
    "fs:default"
  ]
}
